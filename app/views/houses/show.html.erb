  <p id="notice"><%= notice %></p>

  <p>
    <strong>Location:</strong>
    <%= @house.location %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= @house.address %>
  </p>

  <p>
    <strong>House owner:</strong>
    <%= @house.house_owner %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= @house.price %>
  </p>

  <p>
    <strong>Year built:</strong>
    <%= @house.year_built %>
